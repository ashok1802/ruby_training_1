<h1>Book Listing<button><%= link_to 'create new book', 'books/new' %></button></h1>
 
 <table border="1">
	<tr>
		<th>ID</th>
		<th>Title</th>
		<th>Count</th>
		<th>View</th>
		<th>Edit</th>
		<th>Delete</th>
	</tr>
<% @books.each do |b| %>
	<tr>
		<td><%=b.id%> </td>
		<td><%=b.title%> </td>
		<td><%=b.count%> </td>
		<td><%= link_to 'view', book_path(b) %></td>
		<td><%= link_to 'Edit', controller: "books", action: "edit", id: b %></td>
		<td><%= link_to 'Delete', controller: "books", action: "destroy", id: b %></td>
	</tr>

<%end%>
</table>